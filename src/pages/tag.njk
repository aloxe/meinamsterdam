---
layout: base
eleventyExcludeFromCollections: true
eleventyComputed:
    title: 'Mot clé : {{ tag }}'
pagination:
  data: collections
  size: 1
  alias: tag
title: Mot clé {{ tag }}
permalink: /tag/{{ tag }}/
---

<ol>
{% set taglist = collections[ tag ] %}
{% for post in taglist | reverse %}
  <li><a href="{{ post.url }}">{{ post.data.title }}</a></li>
{% endfor %}
</ol>

